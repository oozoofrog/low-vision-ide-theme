  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8A93A6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#87CFA1"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.character.escape",
        "constant.other"
      ],
      "settings": {
        "foreground": "#C3A6FF"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#86B5F5"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class",
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#76C7C0"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "variable.function"
      ],
      "settings": {
        "foreground": "#E0BE7B"
      }
    },
    {
      "scope": [
        "variable.parameter"
      ],
      "settings": {
        "foreground": "#D4DCF1"
      }
    },
    {
      "scope": [
        "variable.other.constant",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#C3A6FF"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#86B5F5"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#B8C0D4"
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#0F1115",
        "background": "#FF5C5C"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.underline",
        "markup.link"
      ],
      "settings": {
        "foreground": "#4AB3FF",
        "fontStyle": "underline"
      }
    }
  ],
  "semanticTokenColors": {
    "comment": {
      "foreground": "#8A93A6",
      "fontStyle": "italic"
    },
    "string": "#87CFA1",
    "number": "#C3A6FF",
    "keyword": "#86B5F5",
    "operator": "#B8C0D4",
    "class": "#76C7C0",
    "interface": "#76C7C0",
    "type": "#76C7C0",
    "enum": "#76C7C0",
    "function": "#E0BE7B",
    "method": "#E0BE7B",
    "property": "#A7C9FF",
    "parameter": "#D4DCF1",
    "variable": "#E6E8EE",
    "enumMember": "#C3A6FF",
    "namespace": "#76C7C0"
  }
