  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5A6473",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#2F6F57"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character.escape", "constant.other"],
      "settings": {
        "foreground": "#7A5A2D"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#1B6FD1"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "support.class", "support.type.builtin"],
      "settings": {
        "foreground": "#0F766E"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call", "variable.function"],
      "settings": {
        "foreground": "#8A5A00"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#344054"
      }
    },
    {
      "scope": ["variable.other.constant", "constant.other.symbol"],
      "settings": {
        "foreground": "#6D5BD0"
      }
    },
    {
      "scope": ["entity.name.tag", "entity.other.attribute-name"],
      "settings": {
        "foreground": "#1B6FD1"
      }
    },
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter", "keyword.operator"],
      "settings": {
        "foreground": "#475467"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#E8E4DF",
        "background": "#B42318"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline", "markup.link"],
      "settings": {
        "foreground": "#1B6FD1",
        "fontStyle": "underline"
      }
    }
  ],
  "semanticTokenColors": {
    "comment": {
      "foreground": "#5A6473",
      "fontStyle": "italic"
    },
    "string": "#2F6F57",
    "number": "#7A5A2D",
    "keyword": "#1B6FD1",
    "operator": "#475467",
    "class": "#0F766E",
    "interface": "#0F766E",
    "type": "#0F766E",
    "enum": "#0F766E",
    "function": "#8A5A00",
    "method": "#8A5A00",
    "property": "#155AA9",
    "parameter": "#344054",
    "variable": "#2D3748",
    "enumMember": "#6D5BD0",
    "namespace": "#0F766E"
  }
}

